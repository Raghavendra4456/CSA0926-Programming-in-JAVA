public class StarTriangle {
    public static void main(String[] args) {
        int K = 5;

        for (int i = 1; i <= K; i++) {
            for (int j = 1; j <= K - i; j++) {
                System.out.print(" ");
            }
            for (int k = 1; k <= i; k++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
